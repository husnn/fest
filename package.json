{
  "name": "fest",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "eslint:check": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "build": "tsc --build tsconfig.build.json",
    "watch": "yarn build --watch",
    "web": "yarn workspace @fest/web",
    "api": "yarn workspace @fest/api",
    "indexer": "yarn workspace @fest/indexer",
    "eth-contracts": "yarn workspace @fest/eth-contracts",
    "console": "yarn workspace @fest/console",
    "house": "yarn workspace @fest/house",
    "merlin": "yarn workspace @fest/merlin"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@yarnpkg/pnpify": "^3.1.1-rc.11",
    "concurrently": "^7.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "kill-port": "^1.6.1",
    "node-gyp": "^8.4.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "express-rate-limit": "^6.2.0"
  }
}
