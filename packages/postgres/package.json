    "build": "tsc --build tsconfig.json",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@fest/core": "workspace:^",
    "@fest/shared": "workspace:^",
    "nanoid": "^3.2.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  }
}
